<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<% if (successMessage) { %>
<script>
  Swal.fire({
    title: "Success!",
    text: "<%= successMessage %>",
    icon: "success",
  });
</script>
<% } %> <% if (errorMessage) { %>
<script>
  Swal.fire({
    title: "Error!",
    text: "<%= errorMessage %>",
    icon: "error",
  });
</script>
<% } %>